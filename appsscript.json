{
  "timeZone": "Asia/Jerusalem",
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "webapp": {
    "executeAs": "USER_ACCESSING",
    "access": "ANYONE"
  },
  "oauthScopes": [
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/spreadsheets.currentonly",
    "https://www.googleapis.com/auth/script.external_request"
  ],
  "urlFetchWhitelist": [
    "https://generativelanguage.googleapis.com/"
  ],
  "addOns": {
    "common": {
      "name": "AI Studio",
      "logoUrl": "https://raw.githubusercontent.com/ilanlal/gas-addon-ai-template/refs/heads/main/assets/icons/48x48.ico",
      "layoutProperties": {
        "primaryColor": "#1010ff",
        "secondaryColor": "#88001b"
      },
      "openLinkUrlPrefixes": [
        "https://raw.githubusercontent.com/ilanlal/",
        "https://www.github.com/ilanlal/",
        "https://github.com/ilanlal/",
        "https://www.easyadm.com",
        "https://easyadm.com"
      ],
      "homepageTrigger": {
        "runFunction": "Addon.Home.Controller.Load"
      },
      "useLocaleFromApp": true,
      "universalActions": [
        {
          "label": "Profile",
          "runFunction": "Addon.UserProfile.Controller.Load"
        },
        {
          "label": "Settings",
          "runFunction": "Addon.Settings.Controller.Load"
        },
        {
          "label": "Help",
          "runFunction": "Addon.Home.Controller.Help"
        },
        {
          "label": "About",
          "runFunction": "Addon.Home.Controller.About"
        },
        {
          "label": "Visit Website",
          "openLink": "https://www.easyadm.com/"
        }
      ]
    },
    "sheets": {}
  }
}